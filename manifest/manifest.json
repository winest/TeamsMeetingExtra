    "privacyUrl": "https://privacy.microsoft.com/en-us/privacystatement/",
    "termsOfUseUrl": "https://www.microsoft.com/en-us/servicesagreement/"
  },
  "name": {
    "short": "Teams Meeting Extra Filters",
    "full": "Teams Meeting Extra Filters"
  },
  "description": {
    "short": "This repo add more filters to camera in Teams Meeting",
    "full": "This repo add more filters to camera in Teams Meeting"
  },
  "icons": {
    "color": "icon-color.png",
    "outline": "icon-outline.png"
  },
  "accentColor": "#FFFFFF",
